// custom

@water:             #1588ec;

@lighten: 50%;
@land: lighten(@land, @lighten);
@grass: lighten(@grass, @lighten);
@beach: lighten(@beach, @lighten);
@park: lighten(@park, @lighten);
@cemetery: lighten(@cemetery, @lighten);
@wooded: lighten(@wooded, @lighten);
@agriculture: lighten(@agriculture, @lighten);

